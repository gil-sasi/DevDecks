{
  "name": "devdecks",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "api:dev": "cd api && npm run dev",
    "api:build": "cd api && npm run build",
    "api:check-db": "cd api && npm run check-db",
    "dev:full": "concurrently \"npm run dev\" \"npm run api:dev\"",
    "build:full": "npm run build && npm run api:build",
    "start": "npm run api:build && concurrently \"npm run preview\" \"cd api && npm start\"",
    "vercel-build": "npm run build:full"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.17.1",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6",
    "typescript": "5.6.2",
    "vite": "^5.0.8"
  }
}
