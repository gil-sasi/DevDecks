{
  "name": "devdecks",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && cd api && npm run build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "api:dev": "cd api && npm run dev",
    "api:build": "cd api && npm run build",
    "api:seed": "cd api && npm run seed",
    "dev:full": "concurrently \"npm run dev\" \"npm run api:dev\"",
    "build:full": "npm run build && npm run api:build",
    "start": "npm run api:build && concurrently \"npm run preview\" \"cd api && npm start\"",
    "vercel-build": "npm run build"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.17.1",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "pinia": "^2.1.7",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6",
    "typescript": "5.6.2",
    "vite": "^5.0.8",
    "vue-tsc": "2.0.29"
  }
}
